#include<stdio.h>
  3 #include<unistd.h>
  4 int main()
  5 {
  6 int ret;
  7 printf("Hi\n");
  8 ret=fork();
  9 if(ret==0)
 10 {
 11 printf("Hello\n"); //Exclusively executed by child process
 12 printf("By.\n");
 13 }
 14 else
 15 {
 16 printf("C prog\n");    //Exclusively executed by parent process
 17 printf("C++ prog\n");
 18 }
 19 }
